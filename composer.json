{
    "name": "laminas-api-tools/api-tools-configuration",
    "description": "Laminas Module providing REST resource for manipulating configuration",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "api-tools",
        "module",
        "rest",
        "config"
    ],
    "homepage": "https://api-tools.getlaminas.org",
    "support": {
        "docs": "https://api-tools.getlaminas.org/documentation",
        "issues": "https://github.com/laminas-api-tools/api-tools-configuration/issues",
        "source": "https://github.com/laminas-api-tools/api-tools-configuration",
        "rss": "https://github.com/laminas-api-tools/api-tools-configuration/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=5.3.3",
        "laminas-api-tools/api-tools-content-negotiation": "dev-master",
        "laminas/laminas": "dev-develop",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "Laminas\\ApiTools\\Configuration\\": "src/",
            "LaminasTest\\ApiTools\\": "test/"
        }
    },
    "replace": {
        "zfcampus/zf-configuration": "self.version"
    }
}
